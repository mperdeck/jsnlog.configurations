<?xml version="1.0"?>
<package >
  <metadata>
    <id>JSNLog.Log4Net</id>
    <version>__Version__</version>
    <title>JSNLog.Log4Net - JavaScript Logging for Log4Net</title>
    <authors>Matt Perdeck</authors>
    <owners>Matt Perdeck</owners>
    <projectUrl>http://jsnlog.com</projectUrl>
    <iconUrl>http://jsnlog.com/Nuget/icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Installs JSNLog and configures it for use with Log4Net. If you use Log4Net and want to log client side JavaScript exceptions and other events in your server log, install this package.</description>
    <copyright>Copyright 2014</copyright>
    <tags>JavaScript logging exceptions Log4Net</tags>
      <dependencies>
	      <!-- Common.Logging.Log4Net1211 does not work with Common.Logging versions lower than 2.2.0 -->
          <dependency id="Common.Logging" version="2.2.0" />
          <dependency id="Common.Logging.Log4Net1211" />
          <dependency id="JSNLog" version="__Version__" />
      </dependencies>
  </metadata>
    <files>
        <file src="web.config.transform" target="content" />
        <file src="..\readme.txt" target="" />
    </files>
</package>
