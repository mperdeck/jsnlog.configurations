<?xml version="1.0"?>
<package >
  <metadata>
    <id>JSNLog.Elmah</id>
    <version>__Version__</version>
    <title>JSNLog.Elmah - JavaScript Logging for Elmah</title>
    <authors>Matt Perdeck</authors>
    <owners>Matt Perdeck</owners>
    <projectUrl>http://jsnlog.com</projectUrl>
    <iconUrl>http://jsnlog.com/Nuget/icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Installs JSNLog and configures it for use with Elmah. If you use Elmah and want to log client side JavaScript exceptions and other events in your server log, install this package.</description>
    <copyright>Copyright 2014</copyright>
    <tags>JavaScript logging exceptions Elmah</tags>
      <dependencies>
	      <!-- Common.Logging.Elmah does not work with Common.Logging versions higher than 2.1.2 -->
          <dependency id="Common.Logging" version="[2.1.2]" />
          <dependency id="Common.Logging.Elmah" />
          <dependency id="JSNLog" />
      </dependencies>
  </metadata>
    <files>
        <file src="..\readme.txt" target="" />
    </files>
</package>
